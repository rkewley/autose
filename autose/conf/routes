# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# Courses Routes
GET     /homeCourses/:id						 controllers.CoursesController.homeCourses(id: Long)
GET     /listCourses						controllers.CoursesController.listCourses
GET		/showCourses/:id					controllers.CoursesController.showCourses(id: Long)
GET 	/editCourses/:id					controllers.CoursesController.editCourses(id: Long)
GET 	/createCourses					controllers.CoursesController.createCourses
POST	/deleteCourses/:id				controllers.CoursesController.deleteCourses(id: Long)
POST 	/saveCourses/:newEntry			controllers.CoursesController.saveCourses(newEntry: Int)


